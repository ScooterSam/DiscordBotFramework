    "mysql": "^2.16.0",
    "node-opus": "^0.2.9",
    "numbro": "^2.1.1",
    "objection": "^1.4.0",
    "relation": "^0.1.0",
    "request": "^2.88.0",
    "require-all": "^2.2.0",
    "simple-youtube-api": "^5.1.1",
    "uws": "^0.14.1",
    "ytdl-core": "^0.29.1"
  }
